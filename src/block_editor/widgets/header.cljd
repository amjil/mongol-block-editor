(ns block-editor.widgets.header
  (:require
   ["package:flutter/material.dart" :as m]
   [cljd.flutter :as f]))

(defn header [{:keys [size color]}]
  (f/widget
   (m/Container
    .margin (m.EdgeInsets/only .left 16)
    .width size
    .height size
    .decoration (m/BoxDecoration.
                 .shape m/BoxShape.circle
                 .color (or color m/Colors.blue.shade200)))))